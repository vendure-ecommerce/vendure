{
    "name": "dev-server",
    "version": "2.2.1-alpha.9",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "populate": "node -r ts-node/register -r dotenv/config populate-dev-server.ts",
        "run:server": "node -r ts-node/register -r dotenv/config index.ts",
        "run:worker": "node -r ts-node/register -r dotenv/config index-worker.ts",
        "start": "concurrently npm:run*",
        "load-test:1k": "node -r ts-node/register load-testing/run-load-test.ts 1000",
        "load-test:10k": "node -r ts-node/register load-testing/run-load-test.ts 10000",
        "load-test:100k": "node -r ts-node/register load-testing/run-load-test.ts 100000"
    },
    "dependencies": {
        "@bb-vendure/admin-ui-plugin": "^2.2.1-alpha.9",
        "@bb-vendure/asset-server-plugin": "^2.2.1-alpha.9",
        "@bb-vendure/common": "^2.2.1-alpha.9",
        "@bb-vendure/core": "^2.2.1-alpha.9",
        "@bb-vendure/elasticsearch-plugin": "^2.2.1-alpha.9",
        "@bb-vendure/email-plugin": "^2.2.1-alpha.9",
        "@nestjs/axios": "^3.0.2",
        "typescript": "5.3.3"
    },
    "devDependencies": {
        "@bb-vendure/testing": "^2.2.1-alpha.9",
        "@bb-vendure/ui-devkit": "^2.2.1-alpha.9",
        "commander": "^12.0.0",
        "concurrently": "^8.2.2",
        "csv-stringify": "^6.4.6",
        "dayjs": "^1.11.10",
        "progress": "^2.0.3"
    },
    "repository": {
        "url": "https://github.com/TheValkDokk/bb-vendure.git",
        "type": "git"
    }
}
