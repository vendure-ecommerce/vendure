        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "5.8.2"
    },
    "dependencies": {
        "@clack/prompts": "^0.11.0",
        "@vendure/common": "3.5.2",
        "commander": "^14.0.1",
