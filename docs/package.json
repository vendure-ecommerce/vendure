{
    "name": "vendure-docs",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./manifest": "./dist/manifest.js"
    },
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "build:manifest": "rimraf dist && tsc -p ./tsconfig.build.json",
        "build": "npm run build:manifest && docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "generate-migration-prompt": "ts-node --compilerOptions '{\"downlevelIteration\":true}' ./scripts/generate-migration-prompt.ts"
    },
    "dependencies": {
        "@docusaurus/core": "^3.7.0",
        "@docusaurus/preset-classic": "^3.7.0",
        "@docusaurus/theme-mermaid": "3.7.0",
        "@mdx-js/react": "^3.0.0",
        "@vendure-io/docs-provider": "^0.2.0",
        "clsx": "^1.2.1",
        "docusaurus-theme-search-typesense": "^0.24.0",
        "prism-react-renderer": "^1.3.5",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "^3.7.0",
        "@tsconfig/docusaurus": "^1.0.5",
        "rimraf": "^5.0.5",
        "typescript": "5.8.2"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
